{
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"pluginSearchDirs": ["."],
	"plugins": ["prettier-plugin-svelte"],
	"printWidth": 100,
	"singleQuote": true,
	"bracketSameLine": true,
	"svelteBracketNewLine": false,
	"trailingComma": "none",
	"useTabs": true
}
