{
	"name": "living-logos",
	"description": "A curation of popular logos, brought to life using HTML, CSS and some fun keyframe animations.",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
