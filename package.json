		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"phosphor-svelte": "^1.2.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"ress": "^5.0.2",
		"sass": "^1.57.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
